{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "20.x",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next && next dev",
    "dev:fresh": "rm -rf .next node_modules/.cache && npm install && next dev",
    "build": "next build",
    "build:clean": "rm -rf .next && next build",
    "start": "cross-env NODE_ENV=production node server.js",
    "start:win": "set \"NODE_ENV=production\" && node server.js",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "audit": "npm audit --audit-level=moderate",
    "update-deps": "npm update && npm audit fix",
    "clear-cache": "rm -rf .next node_modules/.cache",
    "reset": "npm run clear-cache && npm install && npm run dev"
  },
  "dependencies": {
    "@radix-ui/react-slider": "^1.3.5",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "core-js": "^3.38.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.525.0",
    "next": "^14.2.15",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "rc-slider": "^11.1.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icon": "^1.0.0",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5",
    "swapper": "^1.1.6",
    "swiper": "^11.2.10",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.3.5",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "overrides": {
    "glob": "^10.3.10",
    "core-js": "^3.38.1",
    "source-map-js": "^1.2.1"
  },
  "resolutions": {
    "glob": "^10.3.10", 
    "core-js": "^3.38.1",
    "source-map-js": "^1.2.1"
  }
}
